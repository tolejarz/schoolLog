{
    "UserList":{
        "uri":"/user",
        "handler":"UserHandler.list",
        "args":[],
        "method":"get"
    },
    "UserAdd":{
        "uri":"/user/add",
        "reverse_uri":"/user/add",
        "handler":"UserHandler.add",
        "args":["user_id"],
        "method":"get"
    },
    "UserEdit":{
        "uri":"/user/edit/(\\d+)",
        "reverse_uri":"/user/edit/%d",
        "handler":"UserHandler.edit",
        "args":["user_id"],
        "method":"get"
    },
    "UserDelete":{
        "uri":"/user/delete/(\\d+)",
        "reverse_uri":"/user/delete/%d",
        "handler":"UserHandler.delete",
        "args":["user_id"],
        "method":"get"
    }
}
